{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": ["html", "text", "text-summary", "json-summary"],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/test/**",
    "**/tests/**",
    "**/e2e/**",
    "**/node_modules/**",
    "**/.next/**",
    "**/.swc/**"
  ],
  "report-dir": "./coverage/e2e",
  "temp-dir": "./.nyc_output",
  "branches": 0,
  "lines": 0,
  "functions": 0,
  "statements": 0
} 